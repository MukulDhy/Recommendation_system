{
  "name": "rentify",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "installBackend": "npm install express bcryptjs body-parser cloudinary cookie-parser dotenv express-fileupload jsonwebtoken mongoose nodemailer stripe validator colors express-async-handler nodemon mongodb concurrently",
    "installClient": "npm install --prefix frontend",
    "install": "concurrently \"npm run installBackend\" \"npm run installClient\" ",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server \" \"npm run client\" ",
    "ngrok": "ngrok http http://localhost:3000 --host-header=\"localhost:3000\"",
    "liveProduction": "concurrently \"npm run server \" \"npm run client\" \"npm run ngrok\" ",
    "data:import": "node backend/seeder.js",
    "data:destroy": "node backend/seeder.js -d"
  },
  "author": "Mukul Dahiya",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cloudinary": "^2.2.0",
    "colors": "^1.4.0",
    "concurrently": "^8.2.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csv": "^6.3.9",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-async-handler": "^1.2.0",
    "express-fileupload": "^1.5.0",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.6.2",
