    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.13",
    "nodemon": "^3.1.0",
    "stripe": "^15.7.0",
    "universal-cookie": "^7.1.4",
    "validator": "^13.12.0"
  },
  "proxy":"http://localhost:5173"
}
